    "background": {
      "scripts": ["background.js","testing.js","codemirror/lib/codemirror.js","codemirror/mode/xml/xml.js","bootstrap/js/bootstrap.js","bootstrap-select/js/bootstrap-select.js","codemirror/addon/display/autorefresh.js"]
